{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "Root",
    "required": [
        "PodBean",
        "YouTube",
        "Pickle"
    ],
    "properties": {
        "PodBean": {
            "$id": "#/properties/PodBean",
            "type": "object",
            "title": "Podbean",
            "description": "PodBean API information",
            "required": [
                "ClientId",
                "ClientSecret"
            ],
            "properties": {
                "ClientId": {
                    "$id": "#/properties/PodBean/properties/ClientId",
                    "type": "string",
                    "title": "ClientID",
                    "description": "PodBean API Client ID",
                    "default": "",
                    "examples": [
                        ""
                    ],
                    "pattern": "^(.*)$"
                },
                "ClientSecret": {
                    "$id": "#/properties/PodBean/properties/ClientSecret",
                    "type": "string",
                    "title": "ClientSecret",
                    "description": "PodBean API Client Secret",
                    "default": "",
                    "examples": [
                        ""
                    ],
                    "pattern": "^(.*)$"
                }
            }
        },
        "YouTube": {
            "$id": "#/properties/YouTube",
            "type": "object",
            "title": "YouTube",
            "description": "YouTube Configuration",
            "required": [
                "ApiKey",
                "ChannelId",
                "TitlePattern"
            ],
            "properties": {
                "ApiKey": {
                    "$id": "#/properties/YouTube/properties/ApiKey",
                    "type": "string",
                    "title": "Api Key",
                    "description": "YouTube Data v3 API Key",
                    "default": "",
                    "examples": [
                        ""
                    ],
                    "pattern": "^(.*)$"
                },
                "ChannelId": {
                    "$id": "#/properties/YouTube/properties/ChannelId",
                    "type": "string",
                    "title": "Channel ID",
                    "description": "Channel ID of the channel to watch",
                    "default": "",
                    "examples": [
                        ""
                    ],
                    "pattern": "^(.*)$"
                },
                "TitlePattern": {
                    "$id": "#/properties/YouTube/properties/TitlePattern",
                    "type": "string",
                    "title": "Title Pattern",
                    "description": "Regular expression pattern for the title to look for",
                    "default": "",
                    "examples": [
                        "doodle"
                    ],
                    "pattern": "^(.*)$"
                }
            }
        },
        "Pickle": {
            "$id": "#/properties/Pickle",
            "type": "object",
            "title": "Pickle",
            "required": [
                "AccessCode",
                "PublishedAfter"
            ],
            "properties": {
                "AccessCode": {
                    "$id": "#/properties/Pickle/properties/AccessCode",
                    "type": "string",
                    "title": "Accesscode",
                    "default": "",
                    "examples": [
                        "/app/pickles/access_code.pickle"
                    ],
                    "pattern": "^(.*)$"
                },
                "PublishedAfter": {
                    "$id": "#/properties/Pickle/properties/PublishedAfter",
                    "type": "string",
                    "title": "Publishedafter",
                    "default": "",
                    "examples": [
                        "/app/pickles/published_after.pickle"
                    ],
                    "pattern": "^(.*)$"
                }
            }
        }
    }
}
